<% provide(:title, 'すべてのユーザー') %>
<h1>すべてのユーザー</h1>
<%= will_paginate %>
<div class=search_form >
  <%= form_for(users_path, method: :get) do %>
    <%= text_field_tag :search,nil,placeholder: "名前で検索" %>
    <%= submit_tag '検索', :name => nil ,class: 'botan'%>
  <% end %>
</div>
<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>