<% provide(:title, 'すべてのユーザー') %>
<h1>すべてのユーザー</h1>
<%= will_paginate %>
<div class=search_form >
  <%= form_tag(users_path, method: :get) do %>
    <%= text_field_tag :search =>nil %>
    <%= submit_tag '検索', :name => nil ,class: 'botan'%>
  <% end %>
</div>
<ul class="users">
  <% @users.each do |user| %>
    <%= render @users %>
  <% end %>
</ul>

<%= will_paginate %>